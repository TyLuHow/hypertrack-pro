{
  "name": "hypertrack-pro",
  "version": "2.0.0",
  "description": "Evidence-based strength training tracker with Supabase integration",
  "main": "app.js",
  "scripts": {
    "start": "vercel dev",
    "build": "echo 'Static build complete'",
