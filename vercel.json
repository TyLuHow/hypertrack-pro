{
  "version": 2,
  "builds": [
    {
      "src": "hypertrack-pro-v2/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "routes": [
    { "src": "/(.*)\\.(?:js|css|png|jpg|jpeg|gif|ico|svg|webp|json|webmanifest|xml|map|txt|html|woff|woff2|ttf|eot)", "dest": "/$1.$2" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}

